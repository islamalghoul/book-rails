<%= form_with(model: @book, local: true) do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name %>
  
    <%= form.label :release_date %>
    <%= form.date_field :release_date %>
  
    <%= form.label :auther_id %>
    <%= form.collection_select :auther_id, Auther.all, :id, :name %>
  
    <%= form.submit %>
  <% end %>